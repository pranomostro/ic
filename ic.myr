use std
use regex

/*if the input contains the first argument (as a regex)*/
/*return true, otherwise return false*/

const main = {args : byte[:][:]
	if args.len != 2
		std.put("ic REGEX\n")
		std.exit(256)
	;;

	var r = std.slurp("/dev/stdin")
	match r
	| `std.Ok dat:
		match regex.compile(args[1])
		| `std.Ok re:
			std.put("write the regex searching function, idiot\n")
		| `std.Fail msg:
			std.put("ic: {}, exiting.\n", msg)
		;;
	| `std.Fail msg:	std.put("ic: {}, exiting.\n", msg)
	;;
}
